{
  "$schema" : "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest" : "../../dist/ui",
  "lib" : {
    "entryFile" : "src/public-api.ts"
  },
  "assets" : [
    {
      "input" : "styles",
      "glob" : "**/*.scss",
      "output" : "styles"
    },
    {
      "input" : "styles",
      "glob" : "**/*.css",
      "output" : "styles"
    },
    {
      "input" : "styles",
      "glob" : "**/*.ttf",
      "output" : "styles"
    },
    {
      "input" : "styles",
      "glob" : "**/*.woff2",
      "output" : "styles"
    }
  ]
}
